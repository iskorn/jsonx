<jsonx
  xmlns="http://jsonx.openjax.org/jsonx-0.9.8.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://jsonx.openjax.org/jsonx-0.9.8.xsd ../../main/resources/jsonx.xsd">
  <arrayType name="RootArrayBool">
    <boolean/>
  </arrayType>
  <arrayType name="RootMultiArray">
    <array minOccurs="1" maxOccurs="1">
      <array maxOccurs="14">
        <boolean nullable="false"/>
        <number form="integer" range="(1.2,2.4)" maxOccurs="3"/>
      </array>
      <array minOccurs="2" maxOccurs="13">
        <reference type="RootArrayBool" minOccurs="2" nullable="false"/>
        <reference type="RootBooleanNull" maxOccurs="15"/>
      </array>
    </array>
  </arrayType>
  <arrayType name="simple$RootBooleanArray">
    <reference type="RootBooleanNotNull" maxOccurs="3"/>
  </arrayType>
  <booleanType name="RootBooleanNotNull"/>
  <booleanType name="RootBooleanNull"/>
  <numberType name="RootNumber" form="integer" range="(2,9)"/>
  <stringType name="RootString" pattern="[a-z]+" urlEncode="true" urlDecode="true"/>
  <objectType name="cls$AbstractObject" abstract="true">
    <property name="booleanPropertyOptional" xsi:type="reference" type="RootBooleanNull" use="optional"/>
    <property name="booleanPropertyOptionalNotNullable" xsi:type="reference" type="RootBooleanNull" use="optional" nullable="false"/>
    <property name="emptyArray" xsi:type="reference" type="RootArrayBool"/>
  </objectType>
  <objectType name="simple$Booleans">
    <property name="booleanRefDefault" xsi:type="reference" type="RootBooleanNull"/>
    <property name="booleanRefOptional" xsi:type="reference" type="RootBooleanNotNull" use="optional"/>
    <property name="booleanRefOptionalNotNullable" xsi:type="reference" type="RootBooleanNotNull" use="optional" nullable="false"/>
    <property name="booleanDefault" xsi:type="boolean"/>
    <property name="booleanOptional" xsi:type="boolean" use="optional"/>
    <property name="booleanOptionalNotNullable" xsi:type="boolean" use="optional" nullable="false"/>
